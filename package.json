{
  "name": "wallround",
  "version": "1.0",
  "description": "database query builder",
  "main": "index.ts",
  "license": "MIT",
  "engines": {
    "node": ">=22.0.0",
    "yarn": ">=1.22.0"
  },
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --exec npx ts-node src/index.ts",
    "build": "tsc",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "tsc --noEmit",
    "clean": "rm -rf dist",
    "postinstall": "prisma generate",
    "db:deploy": "npx prisma migrate deploy",
    "db:reset": "npx prisma migrate reset --force",
    "db:push": "npx prisma db push",
    "db:seed": "npx ts-node src/utils/seed-data.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.16.2",
    "@types/cors": "^2.8.19",
    "axios": "^1.10.0",
    "body-parser": "^2.2.0",
    "celebrate": "^15.0.3",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "morgan-body": "^2.6.9",
    "prisma": "^6.16.2",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/body-parser": "^1.19.6",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.3",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^11.0.0",
    "jest": "^30.1.3",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}